<script src="./assets/js/slider/sequence-theme.modern-slide-in.js"></script>
<script>
    let galleryThumbs = new Swiper('.gallery-thumbs', {
        effect: 'coverflow',
        grabCursor: true,
        centeredSlides: true,
        slidesPerView: '1',
        // coverflowEffect: {
        //   rotate: 50,
        //   stretch: 0,
        //   depth: 100,
        //   modifier: 1,
        //   slideShadows : true,
        // },
        autoplay: {
            delay: 3000,
            disableOnInteraction: false,
        },

        coverflowEffect: {
            rotate: 0,
            stretch: 0,
            depth: 50,
            modifier: 6,
            slideShadows: false,
        },

    });
    let blog = new Swiper(".swiper-container_1", {
        direction: "horizontal",
        speed: 1000,
        spaceBetween: 50,
        autoplay: {
            delay: 3000,
            disableOnInteraction: false,
        },
        slidesPerView: 1,
        parallax: !0,
        nextButton: false,
        prevButton: false,
        paginationClickable: !0,

        loop: !0
    })
    let a = 1
    $(".day-night").click(() => {
        a++;
        if (a % 2 == 0) {
            $("#ap_soc").attr('src', './assets/img/btrans/soc/ap1.png')
            $("#ins_soc").attr('src', './assets/img/btrans/soc/ins1.png')
            $("#li_soc").attr('src', './assets/img/btrans/soc/li1.png')
            $("#tel_soc").attr('src', './assets/img/btrans/soc/tel1.png')
        } else {
            $("#ap_soc").attr('src', './assets/img/btrans/soc/ap2.png')
            $("#ins_soc").attr('src', './assets/img/btrans/soc/ins2.png')
            $("#li_soc").attr('src', './assets/img/btrans/soc/li2.png')
            $("#tel_soc").attr('src', './assets/img/btrans/soc/tel2.png')
        }
    })
    $('.count').each(function () {
        $(this).prop('Counter', 0).animate({
            Counter: $(this).text()
        }, {
            duration: 10000,
            easing: 'swing',
            step: function (now) {
                $(this).text(Math.ceil(now));
            }
        });
    });
    $('#lightSlider').lightSlider({
        gallery: true,
        item: 1,
        loop: true,
        slideMargin: 0,
        thumbItem: 9
    });
</script>
