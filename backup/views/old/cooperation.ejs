<%- include('header_main.ejs'); -%>

<main class="main-root">
    <div id="dsn-scrollb1ar">
        <header dir="rtl">
            <img src="<%=bnr%>" class="img-fluid" style="width: 100%;">
        </header>


        <div class="wrapper root-contact">

            <div class="image-head p-relative mb-section" style="height: auto !important;">
                <div class="container">
                    <% if(f){ %>
                        <% if(f == 1){ %>
                            <div class="alert alert-success" style="background: green;
    text-align: center;
    width: 180px;
    margin: auto;
    margin-top: 20px;
    padding: 12px;
    color: #fff;
    border-radius: 7px;">با موفقیت ثبت شد</div>
                        <% }else {%>
                        <div class="alert alert-danger" style="background: red;
    text-align: center;
    width: 180px;
    margin: auto;
    margin-top: 20px;
    padding: 12px;
    color: #fff;
    border-radius: 7px;"><%=de%></div>
                        <% } %>
                    <% } %>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-box">
                                <div class=" mt-2 section-title text-center">
                                </div>
                                <form action="/cooperation" method="post">
                                    <div>
                                        <div class="messages"></div>
                                        <div class="input__wrap controls">
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <div class="entry">
                                                            <label style="float:right">* نام شركت - كارگاه </label>
                                                            <input id="form_name" type="text" name="name" placeholder="">
                                                        </div>
                                                        <div class="help-block with-errors"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <div class="entry">
                                                            <label style="float:right">* تلفن و نمابر (با ذكر پيش شماره) </label>
                                                            <input id="form_name" type="text" name="mobile" placeholder="">
                                                        </div>
                                                        <div class="help-block with-errors"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <div class="entry">
                                                            <label style="float:right">آدرس سايت اينترنتي</label>
                                                            <input id="form_name" type="text" name="site" placeholder="">
