<%- include('../header3')-%>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>ویرایش اطلاعات تماس با ما</h1>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <!-- left column -->
                <div class="col-md-12">
                    <%if(s){%>
                        <div class="alert alert-success">
                            با موفقیت ویرایش شد
                        </div>
                    <%}%>
                    <!-- general form elements -->
                    <div class="card card-primary">
                        <div class="card-header">
                            <!--                            <h3 class="card-title">مثال ساده</h3>-->
                        </div>
                        <!-- /.card-header -->
                        <!-- form start -->
                        <form role="form"  method="post" action="/admin/contact_det/edit_<%=d._id%>" >
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="title">دفتر مرکزی</label>
                                    <input type="text" class="form-control" value="<%=d.central_office%>" id="central_office" name="central_office" >
                                </div>
                            </div><div class="card-body">
                                <div class="form-group">
                                    <label for="title">کد پستی</label>
                                    <input type="text" class="form-control" value="<%=d.post_code%>" id="post_code" name="post_code" >
                                </div>
                            </div><div class="card-body">
                                <div class="form-group">
                                    <label for="title">شماره تماس دفتر مرکزی
                                    </label>
                                    <input type="text" class="form-control" value="<%=d.central_office_phone%>" id="central_office_phone" name="central_office_phone" >
                                </div>
                            </div><div class="card-body">
                                <div class="form-group">
                                    <label for="title">فکس</label>
                                    <input type="text" class="form-control" value="<%=d.fax%>" id="fax" name="fax" >
                                </div>
                            </div><div class="card-body">
                                <div class="form-group">
                                    <label for="title">کارخانه زنجان</label>
                                    <input type="text" class="form-control" value="<%=d.zanjan_company%>" id="zanjan_company" name="zanjan_company" >
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="title">کدپستی</label>
                                    <input type="text" class="form-control" value="<%=d.post_code_2%>" id="post_code_2" name="post_code_2" >
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="title">شماره تماس کارخانه زنجان</label>
                                    <input type="text" class="form-control" value="<%=d.zanjan_company_phone%>" id="zanjan_company_phone" name="zanjan_company_phone" >
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="title">فکس</label>
                                    <input type="text" class="form-control" value="<%=d.fax_2%>" id="fax_2" name="fax_2" >
                                </div>
                            </div>
                            <!-- /.card-body -->
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary">ارسال</button>
                            </div>
                        </form>
                    </div>
                    <!-- /.card -->

                    <!-- Form Element sizes -->

                </div>
                <!--/.col (left) -->
                <!-- right column -->
            </div>
            <!-- /.row -->
        </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>
<%- include('../footer')-%>
